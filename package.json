{
  "name": "cmc-mobile",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "test:type": "tsc --noEmit",
    "test:doctor": "npx expo-doctor",
    "prebuild": "npx expo prebuild",
    "build:all": "eas build --platform all",
    "bump-deps": "npx expo install --check",
    "check:biome": "npx @biomejs/biome check --config-path=./biome-ci.json --diagnostic-level=error --write .",
    "ci": "npm run test:type && npm run check:biome && npm run test:doctor && npm run prebuild"
  },
  "dependencies": {
    "@suspensive/react": "^2.18.11",
    "@tanstack/react-query": "^5.64.0",
    "@webview-bridge/react-native": "^1.7.5",
    "@xionwcfm/utils": "^0.0.3",
    "expo": "~52.0.32",
    "expo-clipboard": "~7.0.1",
    "expo-constants": "~17.0.5",
    "expo-device": "~7.0.2",
    "expo-haptics": "~14.0.1",
    "expo-linking": "~7.0.5",
    "expo-notifications": "~0.29.13",
    "expo-splash-screen": "~0.29.21",
    "expo-status-bar": "~2.0.1",
    "expo-store-review": "~8.0.1",
    "expo-web-browser": "~14.0.2",
    "ky": "^1.7.4",
    "react": "18.3.1",
    "react-native": "0.76.7",
    "react-native-gesture-handler": "~2.20.2",
    "react-native-reanimated": "~3.16.1",
    "react-native-safe-area-context": "4.12.0",
    "react-native-screens": "~4.4.0",
    "react-native-web": "~0.19.13",
    "react-native-webview": "13.12.5",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@biomejs/biome": "1.9.4",
    "@commitlint/cli": "^19.6.1",
    "@commitlint/config-conventional": "^19.6.0",
    "@types/react": "~18.3.12",
    "lefthook": "^1.10.3",
    "typescript": "^5.3.3"
  },
  "private": true,
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "listUnknownPackages": false
      }
    }
  }
}
